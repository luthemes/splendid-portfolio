        {
            "name": "Benjamin Lu",
            "email": "benlumia007@gmail.com",
            "homepage": "https://luthemes.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Benlumia007\\SplendidPortfolio\\": "app/"
